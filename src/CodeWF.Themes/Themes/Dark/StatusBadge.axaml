<ResourceDictionary xmlns="https://github.com/avaloniaui" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<!-- 边框颜色 -->
    <SolidColorBrush x:Key="StatusBadgeDebugBorderBrush" Color="#1890FF" />
    <SolidColorBrush x:Key="StatusBadgeInfoBorderBrush" Color="#52C41A" />
    <SolidColorBrush x:Key="StatusBadgeWarnBorderBrush" Color="#FAAD14" />
    <SolidColorBrush x:Key="StatusBadgeErrorBorderBrush" Color="#FF4D4F" />
    <SolidColorBrush x:Key="StatusBadgeFatalBorderBrush" Color="#CF1322" />
    <!-- 更深的红色 -->

    <!-- 背景颜色 -->
    <SolidColorBrush x:Key="StatusBadgeDebugBackground" Color="#E6F7FF" />
    <SolidColorBrush x:Key="StatusBadgeInfoBackground" Color="#F6FFED" />
    <SolidColorBrush x:Key="StatusBadgeWarnBackground" Color="#FFF7E6" />
    <SolidColorBrush x:Key="StatusBadgeErrorBackground" Color="#FFF1F0" />
    <SolidColorBrush x:Key="StatusBadgeFatalBackground" Color="#FFE6E6" />
    <!-- 更饱和的背景色 -->
</ResourceDictionary>
